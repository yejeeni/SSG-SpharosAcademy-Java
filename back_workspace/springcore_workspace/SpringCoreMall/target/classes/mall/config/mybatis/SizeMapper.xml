<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="Size">
<select id="selectAll" resultType="Size">
	select * from size
</select>

	<select id="select" parameterType="int" resultType="Size">
		select * from size where size_id=#{size_id}
	</select>
	
</mapper>
