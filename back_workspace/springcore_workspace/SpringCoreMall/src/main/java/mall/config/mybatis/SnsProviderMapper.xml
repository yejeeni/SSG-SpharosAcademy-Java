<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="SnsProvider">

	 <select id="selectAll" resultType="SnsProvider">
		select * from sns_provider
	</select>

	 <select id="selectByName" parameterType="String" resultType="SnsProvider">
		select * from sns_provider where provider_name = #{provider_name}
	</select>
</mapper>
